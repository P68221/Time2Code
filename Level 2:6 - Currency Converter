def exchange(gbp, currency):
  if currency == "USD":
    money = gbp * 1.3
  elif currency == "Euro":
    money = gbp * 1.11
  elif currency == "Yuan":
    money = gbp * 8.92
  elif currency == "Yen":
    money = gbp * 138.44
  return money

gbp = float(input("Enter the number of Great British Pounds:"))
currency = input("Enter the currency (USD, Euro, Yuan, Yen):")
money = exchange(gbp, currency)
print(gbp, "GBP =", money, currency)
